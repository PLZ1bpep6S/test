{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@encore/*": [
        "libs/*"
      ],
      "authentication/*": [
        "dist/authentication/*"
      ],
      "@encore/authentication": [
        "libs/authentication/src/index.ts"
      ],
      "authorization/*": [
        "dist/authorization/*"
      ],
      "@encore/authorization": [
        "libs/authorization/src/index.ts"
      ],
      "log-service/*": [
        "dist/log-service/*"
      ],
      "@encore/log-service": [
        "libs/log-service/src/index.ts"
      ],
      "pmp-affiliates/*": [
        "dist/pmp-affiliates/*"
      ],
      "@encore/pmp/affiliates": [
        "libs/pmp/affiliates/src/index.ts"
      ],
      "pmp-media/*": [
        "dist/pmp-media/*"
      ],
      "@encore/pmp/media": [
        "libs/pmp/media/src/index.ts"
      ],
      "material/*": [
        "dist/material/*"
      ],
      "@encore/material": [
        "libs/material/src/index.ts"
      ],
      "nrg-levels/*": [
        "dist/nrg-levels/*"
      ],
      "@encore/nrg/levels": [
        "libs/nrg/levels/src/index.ts"
      ],
      "nrg-badges/*": [
        "dist/nrg-badges/*"
      ],
      "@encore/nrg/badges": [
        "libs/nrg/badges/src/index.ts"
      ],
      "pmi-dashboard/*": [
        "dist/pmi-dashboard/*"
      ],
      "@encore/pmi/dashboard": [
        "libs/pmi/dashboard/src/index.ts"
      ],
      "pmi-reports/*": [
        "dist/pmi-reports/*"
      ],
      "@encore/pmi/reports": [
        "libs/pmi/reports/src/index.ts"
      ],
      "dashboard/*": [
        "dist/dashboard/*"
      ],
      "@encore/dashboard": [
        "libs/dashboard/src/index.ts"
      ],
      "nrg-dashboard/*": [
        "dist/nrg-dashboard/*"
      ],
      "@encore/nrg/dashboard": [
        "libs/nrg/dashboard/src/index.ts"
      ]
    }
  }
}
